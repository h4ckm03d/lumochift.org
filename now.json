{
    "version": 2,
    "name": "dev",
    "alias": ["lumochift.org"],
    "builds": [
        {
            "src": "build.sh",
            "use": "@now/static-build",
            "config": { "distDir": "public" }
        }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "headers": {
              "Cache-Control": "public, max-age=86400" 
            },
            "continue": true
        }
    ],
    "public": true
}